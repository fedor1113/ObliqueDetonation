interpolationSchemes
{
    default         linear;
    reconstruct(rho) vanAlbada;
    reconstruct(U)  vanAlbadaV;
    reconstruct(T)  vanAlbada;
    reconstruct(lambda)  vanAlbada;
}

snGradSchemes
{
    default         corrected;
}


